{
  "name": "loopback",
  "version": "0.0.1",
  "description": "training",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14 || 16 || 17 || 18"
  },
  "scripts": {
    "build": "lb-tsc",
    "postinstall": "npm run build",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "server": "npm run build && node . && node dist/index.js",
    "client": "npm start --prefix client",
    "start": "npm run server",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client "
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "ali asgari <aamining@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^9.0.4",
    "@loopback/authentication-jwt": "^0.12.4",
    "@loopback/boot": "^5.0.3",
    "@loopback/core": "^4.0.3",
    "@loopback/repository": "^5.0.3",
    "@loopback/rest": "^12.0.3",
    "@loopback/rest-explorer": "^5.0.3",
    "@loopback/service-proxy": "^5.0.3",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "loopback-connector-mongodb": "^5.6.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.3",
    "@loopback/eslint-config": "^13.0.3",
    "@loopback/testlab": "^5.0.3",
    "@types/node": "^14.18.26",
    "eslint": "^8.22.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.7.4"
  }
}
